{
  "name": "realtime-chat-web-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "encrypt-file": "read -s -p 'üîê Enter password to encrypt: ' pass && echo && openssl aes-256-cbc -salt -pbkdf2 -in FILE_NAME_FROM_ROOT_PATH -out ENCRYPTED_FILE_NAME -k \"$pass\" && echo '‚úÖ file encrypted!'",
    "decrypt-file": "read -s -p 'üîê Enter password to decrypt: ' pass && echo && openssl aes-256-cbc -d -pbkdf2 -in firebaseServiceAccount.json.encrypted -out firebaseServiceAccount.json -k \"$pass\" && echo '‚úÖ file decrypted!'",
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
